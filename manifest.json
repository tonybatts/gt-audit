{
  "manifest_version": 2,
  "name": "Quick Audit",
  "version": "1.0",
  "description": "Quickly run GTmetrix audit",
  "short_name": "Quickly run GTmetrix audit",
  "permissions": [
    "https://gtmetrix.com/",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  }
}